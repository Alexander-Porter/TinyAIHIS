# TinyHIS .env.example
# 后端服务器端口
SERVER_PORT=8080
SILICONFLOW_API_KEY=

# 应用配置
APP_DEMO_MODE=true

# MySQL 配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=tinyhis
MYSQL_USER=root
MYSQL_PASSWORD=root

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# JWT 配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=86400000

# SiliconFlow / DeepSeek

SILICONFLOW_MODEL=deepseek-ai/DeepSeek-V3.2
SILICONFLOW_API_URL=https://api.siliconflow.cn/v1
SILICONFLOW_EMBEDDING_URL=https://api.siliconflow.cn/v1/embeddings


# 业务常量
REGISTRATION_FEE=50.00
REGISTRATION_QUEUE_KEY=registration:queue
QUOTA_KEY_PREFIX=schedule:quota:
SCHEDULE_USERS_PREFIX=schedule:users:
APP_QUEUE_MAX_BATCH_SIZE=50
APP_QUEUE_MAX_RETRY_TIMES=50
APP_QUEUE_POLL_INTERVAL_MS=10



# Frontend (Vite) dev proxy and base URL
VITE_PROXY_TARGET=http://localhost:8080
VITE_PROXY_WS=ws://localhost:8080
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:8080/api
